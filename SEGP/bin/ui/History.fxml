<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderPaneHistory" prefHeight="658.0" prefWidth="946.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HistoryController">
   <left>
      <VBox minHeight="600.0" minWidth="250.0" prefHeight="600.0" prefWidth="250.0" BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="hBox" prefHeight="60.0" prefWidth="250.0">
               <children>
                  <JFXTextField fx:id="search" maxWidth="239.0" minWidth="151.0" onMouseClicked="#SearchDataInTable" prefHeight="31.0" prefWidth="151.0">
                     <HBox.margin>
                        <Insets left="30.0" top="10.0" />
                     </HBox.margin>
                  </JFXTextField>
                  <ImageView fitHeight="27.0" fitWidth="28.0">
                     <image>
                        <Image url="@../../img/search.png" />
                     </image>
                     <HBox.margin>
                        <Insets left="25.0" top="19.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox minWidth="50.0" prefWidth="50.0">
               <children>
                  <JFXButton fx:id="deleteSingle" prefHeight="30.0" prefWidth="100.0" text="Delete">
                     <HBox.margin>
                        <Insets bottom="5.0" left="15.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="btn" onMouseClicked="#deleteHistory" prefHeight="30.0" prefWidth="100.0" text="Truncate">
                     <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
            </HBox>
            <TreeView fx:id="treeView" prefHeight="200.0" prefWidth="198.0" />
         </children>
      </VBox>
   </left>
   <center>
      <JFXTreeTableView fx:id="table" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
